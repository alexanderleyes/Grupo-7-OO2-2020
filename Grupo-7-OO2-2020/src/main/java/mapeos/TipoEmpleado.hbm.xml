<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0 // EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.TipoEmpleado" table="tipoempleado" >
		<id column="idTipoEmpleado" name="idTipoEmpleado">
			<generator class="identity"/>
		</id>	
		<property column="nombre" name="nombre" type="string"/>
		<property column="descripcion" name="descripcion" type="string"/>		
		<property column="sueldoBasico" name="sueldoBasico" type="double"/>						
	</class>
</hibernate-mapping>